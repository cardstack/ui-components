    "ember-multiselect-checkboxes": "^0.11.1",
    "ember-phone-input": "^5.0.0",
    "ember-place-autocomplete": "^2.0.1",
    "ember-power-calendar": "^0.14.5",
    "ember-power-calendar-moment": "^0.1.7",
