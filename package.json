{
  "name": "@cardstack/ui-components",
  "version": "0.2.20",
  "description": "Reusable UI elements for Cardstack projects and Ember apps.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/cardstack/ui-components",
  "license": "MIT",
  "author": "Jen Weber <j@jenweber.dev>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
