    "release-it": "^13.5.2",
    "release-it-lerna-changelog": "^2.1.2"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://cardstack.github.io/ui-components"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
