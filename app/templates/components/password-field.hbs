<TextField
  @type={{this.type}}
  @label={{this.label}}
  @mode={{@mode}}
  @value={{@value}}
  @className="cs-component-email--input"
  @required={{@required}}
  @disabled={{@disabled}}
  @validationMessage={{this.validationMessage}}
  @invalid={{this.invalid}}
  @dataTestName={{this.dataTestName}}
  @handleInput={{action this.handleInput}}
  @theme={{@theme}}
/>