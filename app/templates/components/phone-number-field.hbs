<TextField
  @inputComponent={{this.inputComponent}}
  @type={{this.type}}
  @label={{this.label}}
  @mode={{@mode}}
  @value={{@value}}
  @className={{this.className}}
  @update={{action this.update}}
  @required={{@required}}
  @disabled={{@disabled}}
  @autocomplete="off"
  @autoPlaceholder={{this.autoPlaceholder}}
  @validationMessage={{this.validationMessage}}
  @invalid={{this.invalid}}
  @inputmode="numeric"
  @showLabelInViewMode={{@showLabelInViewMode}}
  @dataTestName={{this.dataTestName}}
  @theme={{@theme}}
/>